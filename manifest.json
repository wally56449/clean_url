{
  "manifest_version": 3,
  "name": "URL 清理器",
  "version": "1.0",
  "description": "還原重定向網址並清除追蹤參數。",
  "permissions": ["tabs", "scripting"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "background": {
    "service_worker": "background.js"
  }
}